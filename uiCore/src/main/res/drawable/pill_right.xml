<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <solid android:color="?legacySecondaryVariant" />

            <stroke android:width="@dimen/strokeSmall2" android:color="?legacyPrimary" />

            <corners android:bottomRightRadius="@dimen/roundingLarge" android:topRightRadius="@dimen/roundingLarge" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape>
            <solid android:color="?legacyPrimary" />

            <stroke android:width="@dimen/strokeSmall2" android:color="?legacyPrimary" />

            <corners android:bottomRightRadius="@dimen/roundingLarge" android:topRightRadius="@dimen/roundingLarge" />
        </shape>
    </item>
</selector>